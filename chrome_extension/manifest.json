{
  "name": "Sharingan",
  "description": "Easily modify deliver preview cookies",
  "version": "2.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "sharingan16.png",
    "48": "sharingan48.png",
    "128": "sharingan128.png"
  },
  "web_accessible_resources": [
    "contentscript.js",
    "sharingan.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "sharingan128.png",
    "default_title": "Enable Sharingan"
  },
  "permissions": [
    "*://*/*",
    "tabs"
  ]
}